{
	"$schema": "https://schema.tauri.app/config/2",
	"productName": "dnhyxc-ai",
	"version": "0.1.0",
	"identifier": "com.dnhyxc.dnhyxc-ai",
	"build": {
		"beforeDevCommand": "pnpm dev",
		"devUrl": "http://localhost:1420",
		"beforeBuildCommand": "pnpm build",
		"frontendDist": "../dist"
	},
	"app": {
		"windows": [
			{
				"label": "main",
				"title": "dnhyxc-ai",
				"width": 1050,
				"height": 720,
				"minWidth": 1050,
				"minHeight": 720,
				"hiddenTitle": true,
				"resizable": true,
				"decorations": true,
				"titleBarStyle": "Overlay",
				"transparent": true,
				"center": true
			}
		],
		"security": {
			"csp": null
		}
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		]
	}
}
