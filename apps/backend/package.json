		"@nestjs/schematics": "^11.0.0",
		"@nestjs/testing": "^11.0.1",
		"@types/bcrypt": "^6.0.0",
		"@types/express": "^5.0.0",
		"@types/jest": "^30.0.0",
		"@types/multer": "^2.0.0",
