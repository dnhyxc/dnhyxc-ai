		"@nestjs/platform-express": "^11.0.1",
		"@nestjs/swagger": "^11.2.3",
		"@nestjs/typeorm": "^10.0.2",
		"argon2": "^0.44.0",
		"bcrypt": "^6.0.0",
		"cache-manager": "^7.2.7",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.3",
		"compressing": "^2.0.0",
		"dotenv": "^17.2.3",
		"express-rate-limit": "^8.2.1",
