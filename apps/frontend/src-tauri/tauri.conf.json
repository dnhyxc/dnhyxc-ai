{
	"$schema": "https://schema.tauri.app/config/2",
	"productName": "dnhyxc-ai",
	"version": "0.0.101",
	"identifier": "com.dnhyxc.dnhyxc-ai",
	"build": {
		"beforeDevCommand": "pnpm dev",
		"devUrl": "http://127.0.0.1:9002",
		"beforeBuildCommand": "pnpm build",
		"frontendDist": "../dist"
	},
	"app": {
		"windows": [
			{
				"label": "main",
				"title": "dnhyxc-ai",
				"width": 1050,
				"height": 720,
				"minWidth": 1050,
				"minHeight": 720,
				"hiddenTitle": true,
				"dragDropEnabled": false,
				"resizable": true,
				"decorations": true,
				"titleBarStyle": "Overlay",
				"center": true,
				"transparent": false
			}
		],
		"security": {
			"csp": null
		}
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"createUpdaterArtifacts": true,
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		]
	},
	"plugins": {
		"updater": {
			"pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEIxM0JDRThDMjM4QUNBREYKUldUZnlvb2pqTTQ3c2RMUlNlR1JnTjJRTjM3YzRkaE9kZXNJTklwZTdjOTRvaTZ6TVZ0aE5ydjYK",
			"endpoints": [
				"https://github.com/dnhyxc/dnhyxc-ai-app/releases/download/latest/latest.json"
			]
		}
	}
}
