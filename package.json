{
	"name": "dnhyxc-ai",
	"version": "0.0.38",
	"description": "",
	"type": "module",
	"scripts": {
		"dev": "pnpm -C apps/frontend tauri dev",
		"build": "pnpm --filter @dnhyxc-ai/scripts bump-version && pnpm -C apps/frontend tauri build && pnpm --filter @dnhyxc-ai/scripts update-latest && pnpm --filter @dnhyxc-ai/scripts upload-to-release",
		"server:dev": "pnpm -C apps/backend start:dev",
		"server:start": "pnpm -C apps/backend start",
		"server:debug": "pnpm -C apps/backend start:debug",
		"server:prod": "pnpm -C apps/backend start:prod",
		"server:build": "pnpm -C apps/backend build",
		"check": "biome check --write",
		"format": "biome format --write",
		"prepare": "husky install",
		"commit": "git-cz",
		"version": "pnpm --filter @dnhyxc-ai/scripts bump-version",
		"latest": "pnpm --filter @dnhyxc-ai/scripts update-latest",
		"upload": "pnpm --filter @dnhyxc-ai/scripts upload-to-release"
	},
	"keywords": [],
	"author": "dnhyxc",
	"license": "ISC",
	"packageManager": "pnpm@10.8.1",
	"devDependencies": {
		"@biomejs/biome": "2.3.9",
		"@commitlint/config-conventional": "^20.2.0",
		"@types/node": "^22.10.7",
		"commitlint": "20.2.0",
		"husky": "8.0.3",
		"ts-node": "^10.9.2",
		"tsx": "^4.21.0",
		"typescript": "~5.8.3"
	},
	"config": {
		"commitizen": {
			"path": "cz-customizable"
		}
	}
}
