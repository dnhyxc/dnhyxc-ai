{
	"extends": "../../tsconfig.json",
	"compilerOptions": {
		"outDir": "./dist",
		"declaration": true,
		"declarationDir": "./dist",
		"emitDeclarationOnly": false,
		"sourceMap": true,
		"noEmit": false,
		"allowImportingTsExtensions": false,
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"experimentalDecorators": false,
		"emitDecoratorMetadata": false
	},
	"include": ["./src/**/*"],
	"exclude": ["node_modules"]
}
